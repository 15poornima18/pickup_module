<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>EmployeeLoginServlet</servlet-name>
        <servlet-class>pickupboy.controller.EmployeeLoginServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>EmployeeLogout</servlet-name>
        <servlet-class>pickupboy.controller.EmployeeLogout</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>EmployeeRegisterServlet</servlet-name>
        <servlet-class>pickupboy.controller.EmployeeRegisterServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>EmployeeLoginServlet2</servlet-name>
        <servlet-class>pickupboy.controller.EmployeeLoginServlet2</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>EmployeeLoginServ2</servlet-name>
        <servlet-class>pickupboy.controller.EmployeeLoginServ2</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>EmployeeEditServlet</servlet-name>
        <servlet-class>pickupboy.controller.EmployeeEditServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>BillServlet</servlet-name>
        <servlet-class>pickupboy.controller.BillServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>EmployeeLoginServlet</servlet-name>
        <url-pattern>/EmployeeLoginServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>EmployeeLogout</servlet-name>
        <url-pattern>/EmployeeLogout</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>EmployeeRegisterServlet</servlet-name>
        <url-pattern>/EmployeeRegisterServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>EmployeeLoginServlet2</servlet-name>
        <url-pattern>/EmployeeLoginServlet2</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>EmployeeLoginServ2</servlet-name>
        <url-pattern>/EmployeeLoginServ2</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>EmployeeEditServlet</servlet-name>
        <url-pattern>/EmployeeEditServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>BillServlet</servlet-name>
        <url-pattern>/BillServlet</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
